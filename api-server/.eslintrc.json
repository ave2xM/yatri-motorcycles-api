{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["google", "prettier"],
  "plugins": ["@typescript-eslint", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "new-cap": "off"
  }
}
