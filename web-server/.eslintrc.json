{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["react-app", "airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "func-names": "off",
    "spaced-comment": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": "off"
  }
}
